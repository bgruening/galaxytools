<macros>
    <xml name="requirements">
        <requirements>
            <requirement type="package" version="1.0">odose</requirement>
            <yield/>
        </requirements>
    </xml>
    <token name="@VERSION@">1.0</token>
    <xml name="stdio">
        <stdio>
            <exit_code range="1:" level="fatal" description="Exit code > 0" />
            <exit_code range=":-1" />
            <regex match="Error:" />
            <regex match="Exception:" />
        </stdio>
    </xml>
    <xml name="citations">
        <citations>
            <citation type="doi">10.1371/journal.pone.0062447</citation>
        </citations>
    </xml>
</macros>
