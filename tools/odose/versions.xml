<tool id="odosenl_versions" name="Software versions" version="@VERSION@">
    <description>for tracking purposes</description>
    <macros>
        <import>macros.xml</import>
    </macros>
    <expand macro="requirements"/>
    <expand macro="stdio"/>
    <command ><![CDATA[
        versions $logfile
    ]]></command>
    <inputs>
    </inputs>
    <outputs>
        <data format="tabular" name="logfile" label="${tool.name}" />
    </outputs>
    <help><![CDATA[
        This tools prints the versions of external software used for provenance purposes.
    ]]></help>
    <expand macro="citations"/>
</tool>
