<?xml version="1.0"?>
<tool name="Tue6071" id="strep_tue6071" tool_type="data_source">
    <description>server</description>
    <command interpreter="python">data_source.py $output $__app__.config.output_size_limit</command>
    <inputs action="http://gbrowse.pharmaceutical-bioinformatics.de/gb2/gbrowse/tue6071/" check_values="false" target="_top"> 
        <display>go to Streptomyces server $GALAXY_URL</display>
        <param name="GALAXY_URL" type="baseurl" value="/tool_runner?tool_id=strep_tue6071" />
    </inputs>
    <request_param_translation>
        <request_param galaxy_name="dbkey" remote_name="dbkey" missing="s002" >
            <value_translation>
                <value galaxy_value="s002" remote_value="tue6071" />
            </value_translation>
        </request_param>
        <request_param galaxy_name="URL" remote_name="URL" missing="">
            <append_param separator="&amp;" first_separator="?" join="=">
                <value name="d" missing="" />
                <value name="dbkey" missing="s002" />
                <value name="q" missing="" />
                <value name="s" missing="" />
                <value name="t" missing="" />
            </append_param>
        </request_param>
        <request_param galaxy_name="URL_method" remote_name="URL_method" missing="post" />
        <request_param galaxy_name="data_type" remote_name="data_type" missing="txt" />
    </request_param_translation>
    <uihints minwidth="800"/>
    <outputs>
        <data name="output" format="txt" />
    </outputs>
    <options sanitize="False" refresh="True"/>
</tool>




