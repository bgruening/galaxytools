    <token name="@VERSION@">1.0.0</token>
    <xml name="version_command">
        <version_command>@EXECUTABLE@ --version</version_command>
    </xml>

    <xml name="stdio">
        <stdio>
            <exit_code range="1:" />
            <exit_code range=":-1" />
            <regex match="Error:" />
            <regex match="Exception:" />
        </stdio>
    </xml>

    <xml name="citations">
        <citations>
            <citation type="doi">10.1101/2024.02.12.579881</citation>
        </citations>
    </xml>
</macros>
