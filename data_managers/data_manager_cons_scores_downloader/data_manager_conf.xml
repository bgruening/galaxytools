<?xml version="1.0"?>
<data_managers>
    <data_manager tool_file="data_manager/data_manager_cons_scores_downloader.xml" id="data_manager_cons_scores_downloader">
        <data_table name="cons_scores_downloader">  <!-- Defines a Data Table to be modified. -->
            <output> <!-- Handle the output of the Data Manager Tool -->
                <column name="value"/>  <!-- columns that are going to be specified by the Data Manager Tool -->
                <column name="name"/>  <!-- columns that are going to be specified by the Data Manager Tool -->
                <column name="path" output_ref="out_file">
                    <move type="file">
                        <source>${path}</source>
                        <target base="${GALAXY_DATA_MANAGER_DATA_PATH}">cons_scores/${path}</target>
                    </move>
                    <value_translation>${GALAXY_DATA_MANAGER_DATA_PATH}/cons_scores/${path}</value_translation>
                    <value_translation type="function">abspath</value_translation>
                </column>
                <column name="version"/>
            </output>
        </data_table>
    </data_manager>
</data_managers>